{# category 4 #}
{% extends '/bundles/lincko/launch/templates/generic/base.twig' %}

{% block content %}
<div id="begin_home" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/launch/scripts/launch/home.js') }}");
		JSfiles.later("{{ _filelatest('/lincko/launch/scripts/launch/mailchimp.js') }}");
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/launch/styles/launch/home.css') }}" type="text/css" />
</div>
<div id="home_main_image"></div>
<div class="home_table_center">
	<span id="home_sentences">
		<img class="home_chinese" src="{{ _filelatest('/lincko/launch/images/launch/chinese_text.png') }}" />
		<span>
			<div>The way of <span class="home_collaboration">collaboration</span></div>
			<div>The way of <span class="home_communication">communication</span></div>
			<div>The way of <span class="home_projects">projects</span></div>
		</span>
	</span>
</div>
<div class="home_footer">
	<img class="icons_diamond maxMobile" src="{{ _filelatest('/lincko/launch/images/launch/icons_diamond.png') }}" />
	<img class="icons_horizontal minMobileL" src="{{ _filelatest('/lincko/launch/images/launch/icons_horizontal_05.png') }}" />
	<div class="home_all_projects">
		All great projects start<span class="maxMobile"><br /></span><span class="minMobileL"> </span>with a single task.
	</div>
	<div class="home_news">
		<form id="home_form" action="//lincko.us13.list-manage.com/subscribe/post?u=ac02b0f4fb881d157df6d3209&amp;id=050321d5ad" method="post" target="_blank" novalidate>
			<label class="home_label_inside" for="home_news_email">
				<span>{{ _trans('web', 4, 21, 'html') }}{# e-mail #}</span>
				<input type="email" id="home_news_email" name="email" value="" />
			</label>
			<label class="minTablet" for="home_news_submit">
				<span id="home_news_submit" onclick="$('#home_form').submit();" />{{ _trans('web', 4, 18, 'input') }}{# Register #}</span>
				<img class="submit_progress_bar" id="home_news_submit_progress" src="{{ _filelatest('/lincko/launch/images/generic/base/generic/loading_bar.gif') }}"/>
			</label>
		</form>
	</div>
	<div id="home_error" class="display_none"></div>
	<div class="home_accomplish">
		<div class="minDesktop">
			{{ _trans('web', 4, 19, 'html') }}{# Lincko helps you accomplish great things by connecting your team in new ways. #}
		</div>
		<div>
			{{ _trans('web', 4, 20, 'html') }}{# Register now to receive news and early access. #}
		</div>
	</div>
</div>
<div id="end_home" class="visibility_hidden">
	{% include '/bundles/lincko/launch/templates/launch/scripts/home.js.twig' %}
</div>
{% endblock %}
