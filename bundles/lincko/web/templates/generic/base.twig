{% extends '/bundles/lincko/wrapper/templates/wrapper.twig' %}
{% block base %}
	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
		ga('create', 'UA-78242020-1', 'auto');
		ga('send', 'pageview');
	</script>
	<script>
		var base_input_field = {};
		JSfiles.later("{{ _filelatest('/lincko/web/scripts/generic/base.js') }}");
		JSfiles.later("{{ _filelatest('/lincko/web/scripts/input/_base.js') }}", 40);
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/web/styles/generic/base.css') }}" type="text/css" />
	{% include '/bundles/lincko/web/templates/generic/styles/base.css.twig' %}
	
	{% include '/bundles/lincko/web/templates/generic/mobile_menu.twig' %}
	{% include '/bundles/lincko/web/templates/generic/account.twig' %}

	<div class="base_wrapper" id="base_wrapper">
		{% include '/bundles/lincko/web/templates/generic/head.twig' %}
		<div class="base_content overthrow" id="base_content">
			<table class="base_table" id="base_table">
				<tr>
					<td class="base_body" id="base_body">
						{% block content %}
						{% endblock %}
					</td>
				</tr>
				<tr>
					<td class="base_foot minTablet">
						{% include '/bundles/lincko/web/templates/generic/foot.twig' %}
					</td>
				</tr>
			</table>
		</div>
	</div>
{% endblock %}
