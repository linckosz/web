{# category 2 #}
{% set route = _route_name() %}
<div id="begin_mobile_menu" class="visibility_hidden">
	<script>
		JSfiles.push("{{ _filelatest('/lincko/web/scripts/generic/mobile_menu.js') }}");
	</script>
	{% include '/bundles/lincko/web/templates/generic/scripts/mobile_menu.js.twig' %}
	<link rel="stylesheet" href="{{ _filelatest('/lincko/web/styles/generic/mobile_menu.css') }}" type="text/css" />
</div>
<div class="display_none">
	<div id="-mobile_menu_wrapper" class="mobile_menu_wrapper maxMobileL">
		<div class="base_menu mobile_menu_top unselectable">
			<span class="mobile_menu_back font_sans_serif"><img class="base_button mobile_menu_back_image" src="{{ _filelatest('/lincko/web/images/generic/mobile_menu/back_w.png') }}" alt="{{ _trans('web', 2, 11, 'html') }}" title="{{ _trans('web', 2, 11, 'html') }}" /></span>
