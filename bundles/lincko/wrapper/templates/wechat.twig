{# category 3 #}
<div id="begin_wrapper_wechat" class="visibility_hidden">
	<script>
		var wrapper_wechat_show_official = false;
		{% if integration_wechat_show_official %}
			wrapper_wechat_show_official = true;
		{% endif %}
		JSfiles.later("{{ _filelatest('/lincko/wrapper/scripts/wechat.js') }}", 40);
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/wrapper/styles/wechat.css') }}" type="text/css" />
	<style>
		#wrapper_wechat_qrcode {
			background-image: url("{{ _filelatest('/lincko/wrapper/images/generic/loading_bar.gif') }}");
		}
	</style>
</div>
<div id="wrapper_wechat_wall">
	<div id="wrapper_wechat_box">
		<div id="wrapper_wechat_info">{{ _trans('wrapper', 3, 2, 'html') }}{# To Improve your Lincko experience, long press to scan the QR code and follow our Official account. ðŸ˜Š #}</div>
		<img id="wrapper_wechat_qrcode" src="{{ _filelatest('/lincko/wrapper/images/wechat/official.jpg') }}" />
		<div id="wrapper_wechat_domain">{{ domain|ucfirst }}</div>
	</div>
	<div id="wrapper_wechat_later">{{ _trans('wrapper', 3, 1, 'html') }}{# Later #}</div>
</div>
<div id="end_wrapper_wechat" class="visibility_hidden"></div>

