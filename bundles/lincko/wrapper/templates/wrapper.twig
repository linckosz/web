<script>

window.name = "brunomartin";

$.cachedScript = function( url ) {
	options = $.extend( {}, {
		dataType: "script",
		cache: true, //Do not need to care here, the cacheability is handle by PHP header
		url: url
	});
	return $.ajax( options );
};


//Global namespace (extend or create)
var Lincko = Lincko || {}; //Create singleton object

//Keep a record of JS files to be launch at the end of the page loading
var JSfiles = [];

$(function() {
	$.cachedScript("{{ _filelatest('/lincko/wrapper/scripts/error.js') }}");
	$.cachedScript("{{ _filelatest('/lincko/wrapper/scripts/language.js') }}");
	$.cachedScript("{{ _filelatest('/translation/list.js') }}").success(function() {
		$.cachedScript("{{ _filelatest('/lincko/wrapper/scripts/wrapper.js') }}").success(function() {
			//alert(Lincko.Translation.get('wrapper', 8000, 0));
			for(var key in JSfiles){
				$.cachedScript(JSfiles[key]);
			}
		});
	});
});


</script>
