<div id="begin_skylist" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/generic/skylist.js') }}");
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/app/styles/generic/skylist.css') }}" type="text/css" />
</div>
<div id="end_skylist" class="visibility_hidden"></div>

<div class="display_none">

	<div id="-skylist_menu_navbar" class="skylist_menu_navbar">
		<span class = "skylist_menu_people">
			<span people="1" class="icon-SinglePerson"></span>
			<span people="2" class="icon-MultiplePeople"></span>
		</span>
		<div class="skylist_menu_timesort"></div>
		<span class="skylist_menu_navbar_search">
			<input find='search_textbox' class="display_none" type='textbox' placeholder="Search Here"></input>
			<span find='search_icon' class="fa fa-search"></span>
		</span>
	</div>
	<span id="-skylist_menu_timesort_text_wrapper" class="skylist_menu_timesort_text_wrapper" sort=''>
		<span find='indicator' class="minTablet"></span>
		<span find='text' class="skylist_menu_timesort_text"></span>
	</span>

	<div id='-skylist_subwrapper' class="skylist_subwrapper"></div>
	<div id="-skylist" class="skylist overthrow"></div>

	<div id="-skylist_card" find="card" class="skylist_card">
		<div find='card_leftOptions' class="skylist_card_leftOptions maxMobileL">
			<div class="skylist_leftOptionBox">
				<div class="fa fa-trash-o"></div>
			</div>
		</div>
		<div find='card_rightOptions' class="skylist_card_rightOptions maxMobileL"></div>
		<table find='card_center' class="skylist_card_center">
			<tr>
				<td find='card_spacestick' class="skylist_card_spacestick">&nbsp;</td>
				<td find='card_leftbox'></td>
				<td find='card_textwrapper' class="skylist_card_textwrapper">
					<div find='title' contenteditable="false"></div><!--<span find='dash'>-</span>-->
					<div find='description'></div>
				</td>
				<span find='card_time' class="skylist_card_time floatright"></span>
			</tr>
		</table>
		<table find="card_bottom" class="skylist_card_bottom">
			<td>
				<span class="fa fa-user"></span>
				<span find='name' class="skylist_icon_text">Name{# toto #}</span><span class="skytest">AAAAA</span>
				<span class="fa fa-pencil"></span>
				<span find='comments' class="skylist_icon_text">(2){# toto #}</span>
			</td>
		</table>
	</div>

	<span id="-skylist_newcardCircle" class="skylist_newcardCircle icon-AddIcon"></span>

	{#rightOptionBox, goes inside <div find='card_rightOptions' #}
	<div id="-skylist_rightOptionBox" class="skylist_rightOptionBox">
		<div find='icon' class="fa"></div>
		<div find='text' class="skylist_rightOptionBox_text">Name{# toto #}</div>
	</div>

	{#for tasklist, goes inside <td find='card_leftbox'></td> #}
	<span id='-skylist_checkbox' class="skylist_checkbox floatleft">
		<input type="checkbox" id="" name="checkname" value="" class="display_none">
		<label for="" class="icon-CheckboxCheck skylist_checkbox_visual"></label>
	</span>


</div>

