{# Category 12 #}
<div id="begin_burger" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/generic/burger.js') }}");
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/app/styles/generic/burger.css') }}" type="text/css" />
</div>
<div id="end_burger" class="visibility_hidden"></div>
<div class="display_none">
	<div id="-burger_dropdown" class="burger_dropdown base_color_bg_neutral" tabindex="1">
		<div find="wrapper" style="position: relative;" ></div>
	</div>
	<div id="-burger_option" class="burger_option">
		<div find="profile_pic"></div>
		<div find="username"></div>
	</div>
	<div id="-burger_tag" class="burger_tag"></div>


	<div id="-burger_calendar_prepend" class="burger_calendar_prepend">
		<div find='today_info'></div>
		<div find='row1'>
			<span find='today_btn' class="skylist_clickable">{{ _trans('app', 8000, 3302, 'html') }}</span>{#today#}
			<span find='tomorrow_btn' class="skylist_clickable">{{ _trans('app', 8000, 3303, 'html') }}</span>{#tomorrow#}
		</div>
		<div find='row2'>
			<span find='twoDays_btn' class="skylist_clickable">{{ _trans('app', 12, 1, 'html') }}</span>{#2 days later#}
			<span find='oneWeek_btn' class="skylist_clickable">{{ _trans('app', 12, 2, 'html') }}</span>{#1 week later#}
		</div>
	</div>

