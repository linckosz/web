{# Category 12 #}
<div id="begin_burger" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/generic/burger.js') }}");
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/app/styles/generic/burger.css') }}" type="text/css" />
</div>
<div id="end_burger" class="visibility_hidden"></div>
<div class="display_none">
	<div id="-burger_dropdown" class="burger_dropdown base_color_bg_neutral" tabindex="1">
		<div find="wrapper" style="position: relative;" ></div>
	</div>
	<div id="-burger_option" class="burger_option needsclick skylist_clickable">
		<div find="image" class="needsclick icon-largerIndividual"></div>
		<div find="text" class="needsclick"></div>
	</div>
	<div id="-burger_tag" class="burger_tag"></div>


	<div id="-burger_calendar_clickHandler" class="burger_calendar_clickHandler" tabindex="0">
	</div>

	<div id="-burger_calendar_prepend" class="burger_calendar_prepend">
		<div find='today_info'></div>

		<table>
			<tr>
				<td>
					<div find='today_btn' dateOffset="0" class="burger_calendar_prepend_btn skylist_clickable">
						{{ _trans('app', 8000, 3302, 'html') }}{#today#}
					</div>
					<div find='tomorrow_btn' dateOffset="1" class="burger_calendar_prepend_btn skylist_clickable">
						{{ _trans('app', 8000, 3303, 'html') }}{#tomorrow#}
					</div>
				</td>
				<td>
					<div find='none_btn' class="burger_calendar_prepend_btn skylist_clickable">
						{{ _trans('app', 8000, 103, 'html') }}{#None#}
					</div>
				</td>
				<td>
					<div find='twoDays_btn' dateOffset="2" class="burger_calendar_prepend_btn skylist_clickable">
						{{ _trans('app', 12, 1, 'html') }}{#2 days later#}
					</div>
					<div find='oneWeek_btn' dateOffset="7" class="burger_calendar_prepend_btn skylist_clickable">
						{{ _trans('app', 12, 2, 'html') }}{#1 week later#}
					</div>
				</td>
			</tr>
		</table>

<!--
		<div find='row1'>
			<span find='today_btn' class="skylist_clickable">{{ _trans('app', 8000, 3302, 'html') }}{#today#}</span>
			<span find='tomorrow_btn' class="skylist_clickable">{{ _trans('app', 8000, 3303, 'html') }}{#tomorrow#}</span>
		</div>
		<div find='row2'>
			<span find='twoDays_btn' class="skylist_clickable">{{ _trans('app', 12, 1, 'html') }}{#2 days later#}</span>
			<span find='oneWeek_btn' class="skylist_clickable">{{ _trans('app', 12, 2, 'html') }}{#1 week later#}</span>
		</div>
-->
	</div>

	<div id="-burger_typeTask" class="burger_typeTask">
		<table find="toggleOpacity" class="burger_typeTask_opacity">
			<tr>
				<td find="checkbox_td">
					<span id="" find="checkbox" class="skylist_checkbox fa fa-check needsclick"></span>
				</td>
				<td class="skylist_card_textwrapper">
					<div find="text" contenteditable="true"></div>
				</td>
			</tr>
		</table>
		<input find="userid" type="number" class="display_none">
	</div>



</div>
