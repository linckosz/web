{# category 6 #}
<div id="begin_models_projects" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/models/projects.js') }}");
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/input/_projects.js') }}", 40);
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/app/styles/models/projects.css') }}" type="text/css" />
	<style>
		.submenu_projects_qrcode_loading {
			background-image: url("{{ _filelatest('/lincko/app/images/generic/base/generic/loading_bar.gif') }}");
		}
	</style>
</div>
<div id="end_models_projects" class="visibility_hidden"></div>

<div class="display_none">

	<div id="-models_projects" class="models_projects_standard base_color_bg_second_less_less">
		<img find="favorite" class="models_projects_standard_favorite" src="{{ _filelatest('/lincko/app/images/models/projects/favorite.png') }}" alt="{{ _trans('app', 5, 1, 'html') }}{# favorite #}" title="{{ _trans('app', 5, 1, 'html') }}{# favorite #}" />
		<div find="title" class="models_projects_standard_title"></div>
		<div class="models_projects_standard_details">
			<div class="models_projects_standard_picture">
				<img find="picture" src="{{ _filelatest('/lincko/app/images/models/projects/project70.png') }}" />
			</div>
			<div class="models_projects_standard_statistics">
				<canvas find="tasks_statistics" width="200" height="80" ></canvas>
			</div>
		</div>
	</div>

	<section id="-app_project_item" class="app_project_item base_MainElementText">
		<div class="app_project_item_stats clear">
			<span find="submenu_projects_statistics_tasks">0</span>
			<span class="icon-Tasks"></span>&nbsp;
			<span find="submenu_projects_statistics_notes">0</span>
			<span class="icon-New-Notes"></span>&nbsp;
			<span find="submenu_projects_statistics_files">0</span>
			<span class="icon-Cloud-Files"></span>
		</div>
		<p></p>
	</section>

	<section id="-app_project_chat_item" class="app_project_chat_item clear">
		<div class="logo_wrapper">
			<img src="" class="logo_img">
			<span class="logo"></span>
			<div class="notification"></div>
		</div>
		<div class="app_project_chat_item_content">
			<header></header>
			<article></article>
		</div>
	</section>

	<canvas id="-models_projects_tasks_statistics" find="tasks_statistics" width="200" height="80" ></canvas>




	<div id="-models_projects_projectsDeck" class="app_models_projects_projectsDeck">
		<div find="menuWrapper">
			<div find="view" class="base_pointer display_none">
				<div find="view_card" class="app_models_projects_projectsDeck_viewOptionBox">
					<span find="icon" class="app_models_projects_projectsDeck_viewIcons icon-cardViewIcon"></span>
					<span>Card{#toto#}</span>
				</div>
				<div find="view_board" class="app_models_projects_projectsDeck_viewOptionBox">
					<span find="icon" class="icon-cardViewIcon"></span>
					<span>Board{#toto#}</span>
				</div>
			</div>
			<div find="searchBar_wrapper"></div>
		</div>
		<div find="deckWrapper">
			<div find="deckWrapper_content">
				<div find="header_recent">{{ _trans('app', 6, 4, 'html') }}{# Recent Projects #}</div>
				<div find="recentWrapper"></div>
				<div find="divider"></div>
				<div find="abcWrapper"></div>
				<div find="btn_archived" class="base_pointer">
					<span find="text">{{ _trans('app', 6, 2, 'html') }}{# Archived Projects #}</span>&nbsp;(<span find="count"></span>)
					<span find="icon" class="icon-Forward"></span>
				</div>
			</div>
		</div>
	</div>

	<div id="-models_projects_card" find="card" class="app_models_projects_projectsDeck_card base_pointer">

		<div find="leftMenu_wrapper">
			<div find="btn_archive" class="icon-Trash"></div>
		</div>

		<div find="cardCenter_wrapper">
			<div class="app_models_projects_projectsDeck_cardCenter_sidePaddingCell"></div>
			<div find="preview">
				<span find="icon" class="icon-projectBlack"></span>
			</div>
			<div find="info">
				<div find="info_activity" class="ellipsis">{{ _trans('app', 6, 3, 'html') }}{# Activity #}:&nbsp;</div>
				<div find="info_main">
					<div find="project_title" class="ellipsis"></div>
					<span find="icon_wrapper">
						<span find="icon_dashboard" class="fa fa-dashboard"></span>
						<span find="icon_settings" class="icon-Settings"></span>
					</span>
				</div>
				<div find="info_progress">
					<div find="bar"><div find="percent"></div></div>
				</div>
			</div>
			<div class="app_models_projects_projectsDeck_cardCenter_sidePaddingCell"></div>
		</div>{# end of cardCenter_wrapper #}

	</div>{# end of models_projects_card #}

</div>
