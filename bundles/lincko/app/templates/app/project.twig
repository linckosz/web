{# category 2 #}
<div id="begin_app_project" class="visibility_hidden">
	<script>
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/models/notification.js') }}", 30);
		JSfiles.later("{{ _filelatest('/lincko/app/scripts/app/project.js') }}", 50);
	</script>
	<link rel="stylesheet" href="{{ _filelatest('/lincko/app/styles/app/project.css') }}" type="text/css" />
</div>
	<div id="app_project_content" class="base_PrimaryMainMenu base_MainElementText app_application_width_child">
		<header id="app_project_top" class="clear">
			<aside class="icon-Settings"></aside>
			<article>{{ title|ucfirst }}</article>
		</header>
		<section id="app_project_placeholder">
			{{ _trans('app', 2, 4, 'html')|ucfirst }}{# Personal Space #}
		</section>
		<section id="app_project_tab">
			<span id="app_project_project_new" class="app_project_header_add icon-Plus-no-circle"></span>
			<header class="clear">
				<div class="app_project_header_stats">
					<span>{{ _trans('app', 2, 5, 'html') }}{# All #}</span>
					<span find="app_project_all_number"></span>
					<span class="icon-Forward"></span>
				</div>
				<p class="sub_header">{{ _trans('app', 2, 13, 'html') }}{# My Projects #}</p>
			</header>
		</section>
		<section id="app_project_chat">
			<header>
				<div class="app_project_header_stats">
					<span>{{ _trans('app', 2, 5, 'html') }}{# All #}</span>
					<span find="app_chat_all_number"></span>
					<span class="icon-Forward"></span>
				</div>
				<p class="sub_header">{{ _trans('app', 2, 14, 'html') }}{# Chats #}</p>
			</header>
		</section>
		<section id="quick_access">
			<span class="icon-Tasks display_none" id="app_project_quick_access_tasks"></span><!--TODO: dangling link-->
			<span class="icon-New-Notes display_none" id="app_project_quick_access_notes"></span><!--TODO: dangling link-->
			<span class="icon-Multipler-Persona" id="app_project_quick_access_chat"></span>
		</section>
	</div>

<div id="end_app_project" class="visibility_hidden"></div>
