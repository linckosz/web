				<span find='time' class="app_layers_dev_skytasks_tasklist_icon_text">Time{# toto #}</span>
			</div>

		</div>
		<div find='task_center' class="app_layers_dev_skytasks_ellipsis">
			<div class="app_layers_dev_skytasks_spacesstick floatleft"></div>
			<span class="app_layers_dev_skytasks_checkbox floatleft">
				<input type="checkbox" id="" name="checkname" value="">
				<div class="app_layers_dev_skytasks_checkbox_visual"></div>
				<label for=""></label>
			</span>
			<span class="app_layers_dev_skytasks_textwrapper">
				<span find='comments' class="app_layers_dev_skytasks_task_comments">
					<img class="app_layers_dev_skytasks_tasklist_icon minTablet" src="{{_filelatest('/lincko/app/images/app/icons/Edition-32.png') }}"/>
					<span class="maxMobileL">Comments{# toto #}</span><span class="">(?)</span>
				</span>
				<span find='title'></span><!--<span find='dash'>-</span>-->
				<span find='description'></span>
			</span>	
		</div>
		<div class="app_layers_dev_skytasks_taskblur"></div>

		<div find='task_leftOptions' class="app_layers_dev_skytasks_leftOptions maxMobileL">
			<div class="app_layers_dev_skytasks_delete">
				DELETE{# toto #}
			</div>
		</div>


	</div>

